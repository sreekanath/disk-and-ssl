sudo apt-get update
sudo apt-get install software-properties-common
sudo add-apt-repository ppa:certbot/certbot
sudo apt-get update

sudo apt-get install python-certbot-apache

sudo certbot --apache -d yufapoker.tk --- for single main domain  cmd

sudo certbot --apache -d yufapoker.tk -d www.yufapoker.tk   -- for sub doamin domain
